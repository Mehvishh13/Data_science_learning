#Numpy are homogeneous means-> store common datatype elements


import numpy as np
arr=np.array([12,33,4,5,6])


arr.dtype


arr.astype('float64')


#downcasting to save memeory
arr_large = np.array([1000000, 2000000, 3000000], dtype=np.int64)
arr_small = arr_large.astype(np.int32)  # Downcasting to a smaller dtype
print(arr_small)  # Output: [1000000 2000000 3000000]
print(arr_small.dtype)  # Output: int32


arr = np.array([1 + 2j, 3 + 4j, 5 + 6j], dtype='complex128')
print(arr)

#If you need to store mixed or complex data types (e.g., Python objects), you can use dtype='object'. However, this type sacrifices performance
arr = np.array([{'a': 1}, [1, 2, 3], 'hello'], dtype=object)
print(arr)



